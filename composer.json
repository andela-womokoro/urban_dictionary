{
    "name": "wilson/cp1",
    "description": "simple urban dictionary",
    "keywords": [
        "wilson",
        "cp1"
    ],
    "homepage": "https://github.com/thephpleague/cp1",
    "license": "MIT",
    "authors": [
        {
            "name": "Wilson Omokoro",
            "email": "wilson.omokoro@andela.com",
            "homepage": "http://github.com/andela-womokoro",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Wilson\\" : "src"
        },
        "classmap": ["src/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Wilson\\Test\\": "tests"
        },
        "classmap": ["tests/"]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}